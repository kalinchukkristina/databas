<%- include("header", {title: title}); %>
<div class="container main">
    <h1>Update product</h1>

    <p>Here you can update a products in database</p>

    <div class="container form">
        <h2>Update entry</h2>
        <% res = res[0] %>
        <form method="post" action="/eshop/update">
            <label>Product ID</label>
            <input type="text" name="p_id" value="<%= res.produktid %>" readonly>
            <label>Name</label>
            <input type="text" name="p_namn" value="<%= res.namn %>">
            <label>Description</label>
            <input type="text" name="p_beskrivning" value="<%= res.beskrivning %>">
            <label>Price</label>
            <input type="number" name="p_pris" value="<%= res.pris %>">
            <label>Picture</label>
            <input type="text" name="p_bild" value="<%= res.bildlank %>">
            <label>Price</label>
            <input type="submit" value="Update">
        </form>
    </div>
</div>


<%- include("footer"); %>